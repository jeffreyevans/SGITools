citHeader("To cite SGITools in publications use:")

sagegrouse.ref <- c(
   bibentry(
     bibtype = "Manual",
     title = "SGITools",
     author = person(c("Jeffrey", "S."), "Evans", role = c("aut", "cre")),
     year = "2015",
     note = "R package version 0.1-0",
   )
)

